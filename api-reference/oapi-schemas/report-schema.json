  "paths": {
    "/v1/report/executions/csv": {
      "post": {
        "tags": [
          "Report"
        ],
        "summary": "Download executions CSV",
        "description": "Downloads executions as a CSV file stream",
        "operationId": "ReportAPI_DownloadExecutions",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DownloadExecutionsRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "A successful response.(streaming responses)",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "result": {
                      "$ref": "#/components/schemas/DownloadExecutionsResponse"
                    }
                  },
                  "title": "Stream result of v1DownloadExecutionsResponse"
                }
              }
            }
          }
        }
      }
    },
    "/v1/report/executions/search": {
      "post": {
        "tags": [
          "Report"
        ],
        "summary": "Search executions",
        "description": "Searches for exchange executions using the given details to filter",
        "operationId": "ReportAPI_SearchExecutions",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
