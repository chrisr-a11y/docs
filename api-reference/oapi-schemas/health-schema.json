{
  "openapi": "3.0.1",
  "info": {
    "title": "Health API",
    "version": "v1.0.0"
  },
  "servers": [
    {
      "url": "https://api.polymarketexchange.com"
    }
  ],
  "tags": [
    {
      "name": "HealthAPI"
    }
  ],
  "paths": {
    "/v1/health": {
      "get": {
        "tags": [
          "Health"
        ],
        "summary": "Health check",
        "description": "Check service health status. Returns 200 OK when the service is healthy.",
        "operationId": "HealthAPI_HealthCheck",
        "responses": {
          "200": {
            "description": "Service is healthy",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HealthCheckResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "HealthCheckResponse": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "description": "Health status (e.g., 'ok')"
          }
        }
      }
    }
  }
}
